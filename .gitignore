# Gitignore for Next.js (with Bun, Tailwind, Biome)

# Dependencies
/node_modules
/.bun
/bun.lockb

# Next.js production build output
/.next/
/out/

# Production dependencies (npm-debug.log)
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*
.pnpm-error.log*

# Biome ignores
# Biome's own cache
/.biome/

# Environment variables
.env.local
.env.development.local
.env.test.local
.env.production.local

# Next.js specific build files
.next/
out/
.next/
!/.next/static # Keep static assets often deployed

# Runtime data
/tmp
.turbo/

# Editor/IDE specific files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Logs
*.log
npm-debug.log*
yarn-debug.log*

# Local data (e.g., SQLite databases)
*.sqlite
*.sqlite3

# Test coverage
/coverage

# Vercel specific (if not handled by .vercel/output)
.vercel/output/

# Mac specific files
.DS_Store