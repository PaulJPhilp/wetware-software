# Copy to .env.local and fill in values. Do NOT commit .env.local

# Notion
NOTION_API_KEY=xxxxxx
NOTION_DATABASE_ID_BLOG_POSTS=xxxxxx
NOTION_DATABASE_ID_SERIES=xxxxxx

# Optional: GitHub API Token (increases rate limit from 60 to 5000 requests/hour)
# Create a personal access token at https://github.com/settings/tokens
# Only needs 'public_repo' scope for reading public repository data
GITHUB_TOKEN=your_github_personal_access_token_here

# Optional SDK keys (do not commit real values)
OPENAI_API_KEY=sk-xxxx
ANTHROPIC_API_KEY=sk-ant-xxxx

# App Settings
NEXT_PUBLIC_FEATURED_SERIES_SLUG=
STRICT_SERIES_COVERS=0
SERIES_WARN_COVERS=0
